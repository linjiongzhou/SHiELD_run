	#20190911.12Z 20190912.12Z 20190913.12Z 20190914.12Z 20190915.12Z 20190916.12Z 20190917.12Z 20190918.12Z 20190919.12Z 20190920.12Z \
	#20190921.12Z 20190922.12Z 20190923.12Z 20190924.12Z 20190925.12Z 20190926.12Z 20190927.12Z 20190928.12Z 20190929.12Z 20190930.12Z \
	)
	set runname = SHiELD_${title}${MEMO}.${DATE}
	echo $runname
	cp -f $basescript ${runname}.csh
	sbatch --job-name=${runname} --account=${account} --qos=${qos} --cluster=${cluster} --nodes=${nodes} --export=NAME=${DATE},MEMO=${MEMO},EXE=${EXE},LX=${LX},ALL ${runname}.csh
end

